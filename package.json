{
  "name": "arango-migrate",
  "version": "0.0.3",
  "description": "Migration tools for ArangoDB",
  "author": "Tim Mikeladze <tim.mikeladze@gmail.com>",
  "keywords": [
    "arango",
    "arangodb",
    "arango migrations",
    "migration"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TimMikeladze/arango-migrate.git"
  },
  "license": "MIT",
  "files": [
    "./dist"
  ],
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "exports": "./dist/index.modern.js",
  "bin": "./dist/cli.js",
  "scripts": {
    "dev": "microbundle watch src/{index,cli}.ts",
    "build": "microbundle src/{index,cli}.ts",
    "lint": "eslint --fix {src,__tests__}/**/*.ts",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "dotenv": "10.0.0",
    "eslint": "8.6.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.5",
    "microbundle": "0.14.2",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "arangojs": "7.6.1",
    "commander": "8.3.0",
    "glob": "7.2.0",
    "slugify": "1.6.5"
  }
}
